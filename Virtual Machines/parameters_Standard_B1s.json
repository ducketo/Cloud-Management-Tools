{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "southeastasia"
        },
        "networkInterfaceName": {
            "value": "VM1-nic"
        },
        "networkSecurityGroupName": {
            "value": "VM-nsg"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "RDP",
                    "properties": {
                        "priority": 300,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "3389"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "default"
        },
        "virtualNetworkName": {
            "value": "VM-vnet"
        },
        "addressPrefixes": {
            "value": [
                "10.1.1.0/24"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "default",
                    "properties": {
                        "addressPrefix": "10.1.1.0/24"
                    }
                }
            ]
        },
        "publicIpAddressName": {
            "value": "VM1-ip"
        },
        "publicIpAddressType": {
            "value": "Dynamic"
        },
        "virtualMachineName": {
            "value": "VM1"
        },
        "virtualMachineRG": {
            "value": "ARMdeploy"
        },
        "osDiskType": {
            "value": "Premium_LRS"
        },
        "virtualMachineSize": {
            "value": "Standard_B1s"
        },
        "adminUsername": {
            "value": "VM1_admin"
        },
        "adminPassword": {
            "value": "xxxxxx"
        }
    }
}